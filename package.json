{
  "name": "percentage-change",
  "version": "3.0.0",
  "description": "Binary operator of percentage-change",
  "main": "dist/src/percentage-change.js",
  "types": "dist/src/percentage-change.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf -- dist",
    "ava": "ava -v",
    "ava:fail-fast": "ava -v --fail-fast",
    "ava:serial": "ava -v -s",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --ext ts --fix .",
    "format": "prettier --list-different .",
    "format:fix": "prettier --write .",
    "watch:lint": "nodemon -e 'ts js json' -x 'npm run lint || true'",
    "test": "nyc ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strong-roots-capital/percentage-change.git"
  },
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/EricCrosson)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/percentage-change/issues"
  },
  "homepage": "https://github.com/strong-roots-capital/percentage-change",
  "dependencies": {
    "fp-ts": "^2.8.5"
  },
  "devDependencies": {
    "@ericcrosson/eslint-config": "3.0.0",
    "@ericcrosson/prettier-config": "1.0.0",
    "@semantic-release/git": "10.0.1",
    "@types/node": "16.11.36",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "ava": "4.2.0",
    "ava-fast-check": "5.0.0",
    "codecov": "3.8.2",
    "eslint": "8.16.0",
    "eslint-plugin-ava": "13.2.0",
    "eslint-plugin-security": "1.5.0",
    "fast-check": "2.25.0",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "semantic-release": "19.0.2",
    "ts-node": "10.8.0",
    "ts-pattern": "4.0.2",
    "typescript": "4.7.2"
  },
  "files": [
    "dist/src/"
  ],
  "ava": {
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [
    "percentage",
    "change",
    "operator"
  ]
}
