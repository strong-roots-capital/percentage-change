{
  "name": "percentage-change",
  "version": "3.0.0",
  "description": "Binary operator of percentage-change",
  "keywords": [
    "percentage",
    "change",
    "operator"
  ],
  "homepage": "https://github.com/strong-roots-capital/percentage-change",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/percentage-change/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strong-roots-capital/percentage-change.git"
  },
  "license": "ISC",
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/EricCrosson)",
  "main": "dist/src/percentage-change.js",
  "types": "dist/src/percentage-change.d.ts",
  "files": [
    "dist/src/"
  ],
  "scripts": {
    "ava": "ava -v",
    "build": "tsc",
    "clean": "rm -rf -- dist",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "format": "prettier --list-different .",
    "format:fix": "prettier --write .",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --ext ts --fix .",
    "test": "nyc ava"
  },
  "dependencies": {
    "fp-ts": "^2.8.5"
  },
  "devDependencies": {
    "@ericcrosson/eslint-config": "3.0.0",
    "@ericcrosson/prettier-config": "1.0.0",
    "@semantic-release/git": "10.0.1",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "ava": "5.0.1",
    "ava-fast-check": "6.0.0",
    "codecov": "3.8.2",
    "eslint": "8.26.0",
    "eslint-plugin-ava": "13.2.0",
    "eslint-plugin-security": "1.5.0",
    "fast-check": "3.3.0",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.1",
    "ts-pattern": "4.0.5",
    "typescript": "4.8.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
